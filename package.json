    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-gigigo-standard": "^0.1.9",
    "express": "^4.15.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "nyc": "^10.3.2",
    "opn-cli": "^3.1.0",
